<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".reservarcomidaymesa">

    <ScrollView
        android:id="@+id/subirybajarpag"
        android:layout_width="411dp"
        android:layout_height="479dp"
        android:layout_marginBottom="252dp"
        android:fillViewport="true"
        android:scrollbarSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="411dp"
                android:layout_height="800dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0">

                <Spinner
                    android:id="@+id/spinnerpeople"
                    android:layout_width="147dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="36dp"
                    android:layout_marginEnd="76dp"
                    android:contentDescription="@string/selec_personas"
                    android:minHeight="48dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txtvertime" />

                <TextView
                    android:id="@+id/txtpeople"
                    android:layout_width="110dp"
                    android:layout_height="28dp"
                    android:layout_marginStart="48dp"
                    android:layout_marginTop="36dp"
                    android:text="@string/personas"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/selecttime" />

                <TextView
                    android:id="@+id/txtvertime"
                    android:layout_width="107dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="60dp"
                    android:background="@drawable/common_google_signin_btn_icon_dark_normal_background"
                    android:text="@string/hora"
                    android:textAlignment="center"
                    android:textAllCaps="false"
                    android:textIsSelectable="false"
                    android:textSize="20sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txtvercalendar" />

                <Button
                    android:id="@+id/selecttime"
                    android:layout_width="178dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginTop="12dp"
                    android:onClick="abrirtime"
                    android:text="@string/selecionar_hora"
                    android:textSize="12sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/selfech" />

                <TextView
                    android:id="@+id/txtvercalendar"
                    android:layout_width="107dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="32dp"
                    android:layout_marginEnd="60dp"
                    android:background="@drawable/common_google_signin_btn_icon_dark_normal_background"
                    android:text="@string/fecha"
                    android:textAlignment="center"
                    android:textAllCaps="false"
                    android:textIsSelectable="false"
                    android:textSize="20sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/selfech"
                    android:layout_width="178dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginTop="32dp"
                    android:onClick="abrirCalendar"
                    android:text="@string/seleccionar_fecha"
                    android:textSize="12sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="OnClick" />

                <TextView
                    android:id="@+id/com1"
                    android:layout_width="74dp"
                    android:layout_height="31dp"
                    android:layout_marginTop="76dp"
                    android:layout_marginEnd="64dp"
                    android:text="@string/agua"
                    android:textSize="20sp"
                    app:layout_constraintEnd_toStartOf="@+id/an1"
                    app:layout_constraintTop_toBottomOf="@+id/txtpeople" />

                <Button
                    android:id="@+id/an1"
                    android:layout_width="90dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="48dp"
                    android:layout_marginEnd="108dp"
                    android:text="@string/a_adir"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinnerpeople"
                    tools:ignore="DuplicateSpeakableTextCheck" />

                <TextView
                    android:id="@+id/com2"
                    android:layout_width="74dp"
                    android:layout_height="31dp"
                    android:layout_marginTop="44dp"
                    android:layout_marginEnd="64dp"
                    android:text="@string/patatas"
                    android:textSize="20sp"
                    app:layout_constraintEnd_toStartOf="@+id/an2"
                    app:layout_constraintTop_toBottomOf="@+id/com1" />

                <Button
                    android:id="@+id/an2"
                    android:layout_width="90dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginEnd="76dp"
                    android:text="@string/a_adir"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="@+id/an1"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/an1"
                    app:layout_constraintTop_toBottomOf="@+id/an1" />

                <TextView
                    android:id="@+id/com3"
                    android:layout_width="74dp"
                    android:layout_height="31dp"
                    android:layout_marginTop="44dp"
                    android:text="@string/nuggets"
                    android:textSize="20sp"
                    app:layout_constraintEnd_toStartOf="@+id/an3"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/com2"
                    app:layout_constraintTop_toBottomOf="@+id/com2" />

                <Button
                    android:id="@+id/an3"
                    android:layout_width="90dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="28dp"
                    android:text="@string/a_adir"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="@+id/an2"
                    app:layout_constraintStart_toStartOf="@+id/an2"
                    app:layout_constraintTop_toBottomOf="@+id/an2" />

                <TextView
                    android:id="@+id/com4"
                    android:layout_width="74dp"
                    android:layout_height="31dp"
                    android:layout_marginTop="44dp"
                    android:text="@string/finguers"
                    android:textSize="20sp"
                    app:layout_constraintEnd_toStartOf="@+id/an4"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/com3"
                    app:layout_constraintTop_toBottomOf="@+id/com3" />

                <Button
                    android:id="@+id/an4"
                    android:layout_width="90dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="28dp"
                    android:text="@string/a_adir"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="@+id/an3"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/an3"
                    app:layout_constraintTop_toBottomOf="@+id/an3" />

                <TextView
                    android:id="@+id/com5"
                    android:layout_width="74dp"
                    android:layout_height="31dp"
                    android:layout_marginTop="44dp"
                    android:text="@string/fanta"
                    android:textSize="20sp"
                    app:layout_constraintEnd_toStartOf="@+id/an5"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/com4"
                    app:layout_constraintTop_toBottomOf="@+id/com4" />

                <Button
                    android:id="@+id/an5"
                    android:layout_width="90dp"
                    android:layout_height="48dp"
                    android:layout_marginTop="28dp"
                    android:text="@string/a_adir"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="@+id/an4"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/an4"
                    app:layout_constraintTop_toBottomOf="@+id/an4" />

                <Button
                    android:id="@+id/btnreservamesaycomida"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="56dp"
                    android:text="@string/hacer_reserva"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.766"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/an5" />

                <TextView
                    android:id="@+id/txttttt"
                    android:layout_width="42dp"
                    android:layout_height="47dp"
                    android:layout_marginTop="240dp"
                    android:visibility="invisible"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.484"
                    app:layout_constraintStart_toEndOf="@+id/btnreservamesaycomida"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/btnatrss"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="72dp"
                    android:layout_marginTop="72dp"
                    android:text="@string/atr_s"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/com5" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <ListView
        android:id="@+id/listacomidaselect"
        android:layout_width="match_parent"
        android:layout_height="243dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>